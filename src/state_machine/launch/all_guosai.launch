<launch>
    <!-- 导航模块 -->
    <include file="$(find ucar_nav)/launch/base_ekf.launch" />

    <!-- 自适应蒙特卡洛定位 (AMCL) -->
    <include file="$(find ucar_nav)/config/amcl_test/amcl_origin.launch" />
    
    <!-- 语音识别模块 -->
    <include file="$(find xf_mic_asr_offline)/launch/xf_mic_asr_offline.launch" />

    <!-- USB摄像头模块 -->
    <include file="$(find lby_usb_cam)/launch/usb_cam_noimg_10hz.launch" />
    
    <!-- 二维码识别模块 -->
    <include file="$(find qr_code)/launch/qr_code.launch" />

    <!-- YOLO 节点 -->
    <include file="$(find darknet_ros)/launch/darknet_ros.launch" />
    
    <!-- 语音播报节点 -->
    <node pkg="xf_mic_asr_offline" type="voice_player.py" name="voice_player_node" output="screen" />

    <!-- 状态机节点 -->
    <node pkg="state_machine" type="all_guosai.py" name="state_machine_node" output="screen" />

</launch>